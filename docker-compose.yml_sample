version: '2'
services:
  web:
    build: .
    image: culturagovbr/salic-web:1.2
    container_name: salic-webv1.2
    ports:
     - "80:80"
     - "8888:8888"
    volumes:
     - /var/www/salic:/var/www/salic
     - /var/www/log/apache2/salic:/var/log/apache2
     - /var/www/salic/public/txt:/var/www/salic/public/txt/
     - /var/www/salic/public/plenaria:/var/www/salic/public/plenaria/
    environment:
     - APPLICATION_ENV=development